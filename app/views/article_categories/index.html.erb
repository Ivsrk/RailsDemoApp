<h1>Listing all Article- categories</h1>
<table>
  <thead>
    <tr>
    <th>Id</th>
      <th>Artcle-Id</th>
      <th>Category-Id</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
<% @article_categories.each do |article_category| %>
    <tr>
    <td><%= article_category.id %></td>
        <td><%= link_to article_category.article_id,article_category_path(article_category.id)%></td>
         <td><%= link_to article_category.category_id ,article_category_path(article_category.id)%></td>     
        <td><%= link_to 'delete' , article_category ,method: :delete, data: { confirm: 'Are you sure?' }%></td>
        <td><%= link_to 'edit' , edit_article_category_path(article_category)%></td>
        
    </tr>
<%end%>
<%= link_to 'Create' , new_article_category_path%>
</tbody>

</table>

<br>
