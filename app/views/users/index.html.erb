<h1>Listing all users</h1>
<table>
  <thead>
    <tr>
    <th>Id</th>
      <th>Name</th>
      <th>email</th>
      
      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
<% @users.each do |user| %>
    <tr>
    <td><%= user.id %></td>
        <td><%= link_to user.name ,user_path(user.id)%></td>
        <td><%= user.email %></td>
        
        <td><%= link_to 'delete' , user ,method: :delete, data: { confirm: 'Are you sure?' }%></td>
        <td><%= link_to 'edit' , edit_user_path(user)%></td>
        
    </tr>
<%end%>
<%= link_to 'Create' , new_user_path%>
</tbody>

</table>

<br>
